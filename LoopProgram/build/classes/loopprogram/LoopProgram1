package loopprogram;

import java.util.Scanner;


public class LoopProgram {

    
    public static void main(String[] args) {
        
        for(int i=0; i< 5; i++){
            
        
        
       String name = "Jasbir";
       
       String address = "Sydney";
       
       String Subject = "Java";
       
       String Role = "Student";
       
       String Weather = "25  degree celcius";
       
       String date = "23.11.2020";
       
      
        System.out.println("My name is:" + name);
        System.out.println("My address is:" +address);
        System.out.println("My subject is:" +Subject);
        System.out.println("My role is:" +Role);
        System.out.println("Today's weather is:" +Weather);
        System.out.println("Today's date is:" +date);
            System.out.println(".........................");
        }
        
    }
    
}
